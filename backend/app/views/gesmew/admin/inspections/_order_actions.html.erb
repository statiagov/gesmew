<% content_for :page_actions do %>
  <% if can?(:fire, inspection) %>
    <%= event_links(inspection, events) %>
  <% end %>
  <% if can?(:resend, inspection) %>
    <%= button_link_to Gesmew.t(:resend, scope: 'admin.inspection.events', default: Gesmew.t(:resend)),
          resend_admin_order_url(inspection),
          method: :post,
          icon: 'envelope'
    %>
  <% end %>
<% end %>
