<% content_for :page_title do %>
  <%= Gesmew.t(:new_taxonomy) %>
<% end %>

<%= form_for [:admin, @taxonomy] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <div class="form-actions" data-hook="buttons">
      <%= button Gesmew.t(:create), 'save' %>
    </div>
  </fieldset>
<% end %>
