<% content_for :page_title do %>
  <%= Gesmew.t(:new_user) %>
<% end %>

<div data-hook="admin_user_new_form_header">
  <%= render :partial => 'gesmew/admin/shared/error_messages', :locals => { :target => @user } %>
</div>

<div data-hook="admin_user_new_form">
  <%= form_for [:admin, @user], url: gesmew.admin_users_url, method: :post do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

    <div data-hook="admin_user_new_form_buttons">
      <%= render :partial => 'gesmew/admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
