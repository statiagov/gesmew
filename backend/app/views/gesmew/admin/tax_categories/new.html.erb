<% content_for :page_title do %>
  <%= Gesmew.t(:new_tax_category) %>
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'arrow-left', Gesmew.t(:back_to_resource_list, resource: Gesmew::TaxCategory.model_name.human), admin_tax_categories_path, class: 'btn btn-default' %>
<% end %>

<%= render :partial => 'gesmew/admin/shared/error_messages', :locals => { :target => @tax_category } %>

<%= form_for [:admin, @tax_category] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'gesmew/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
